{
	"name" : "wscl/wscl-main-plugin",
	"description" : "Customization plugin for washingtonleague.org",
	"type" : "library",
	"version" : "2.0.5",
	"minimum-stability" : "stable",
	"require" : {
		"php" : ">=8.2",
		"composer/installers" : "~2.3",
		"deliciousbrains/wp-background-processing" : "~1.4",
		"psr/log" : "~3.0",
		"monolog/monolog" : "~3.9",
		"guzzlehttp/guzzle" : "~7.9",
		"guzzlehttp/uri-template" : "~1.0",
		"json-mapper/json-mapper" : "~2.25",
		"league/csv" : "~9.24",
		"mailerlite/mailerlite-api-v2-php-sdk" : "~0.3",
		"php-di/php-di" : "~7.1"
	},
	"require-dev" : {
		"phpstan/phpstan" : "^2.1",
		"phpstan/extension-installer" : "~1.4",
		"szepeviktor/phpstan-wordpress" : "~2.0",
		"phpunit/phpunit" : "~11.5",
		"brain/monkey" : "~2.6"
	},
	"autoload" : {
		"classmap" : [
			"src/"
		],
		"psr-4" : {
			"" : "src"
		}
	},
	"config" : {
		"allow-plugins" : {
			"composer/installers" : true,
			"php-http/discovery" : true,
			"phpstan/extension-installer" : true
		}
	}
}